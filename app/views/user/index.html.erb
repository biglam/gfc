<h2>Users</h2>
<ul>
<% @users.each do |user| %>
<li><%= user.email %></li>
<% end %>
</ul>
<h2>Current Standings</h2>
<table>
  <tr>
    <th>User</th>
    <th>Points</th>
  </tr>
<% @users.order(points: :desc).each do |user| %>

  <tr>
    <td><%= user.email %></td>
    <td><%= user.points %></td>
  </tr>
  <% end %>
</table>

<h1>ISSUE A CHALLENGE!</h1>